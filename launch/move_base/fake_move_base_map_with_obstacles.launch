<?xml version="1.0"?>

<launch>

<!-- https://github.com/pirobot/rbx1/blob/indigo-devel/rbx1_nav/launch/fake_move_base_map_with_obstacles.launch  -->

  <!-- Run the map server with a blank map + obstacle -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find robot_sensors)/maps/blank_map_with_obstacle.yaml"/>


  <include file="$(find robot_sensors)/launch/move_base/fake_move_base_obstacles.launch" />
  

 
  <!-- Run a static transform between /odom and /map -->
  <node pkg="tf" type="static_transform_publisher" name="odom_map_broadcaster" args="0 0 0 0 0 0 /map /odom 100" />

</launch>
