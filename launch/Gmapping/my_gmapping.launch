<?xml version="1.0"?>

<launch>
    <!-- Launch file parameters -->

    <arg name="output"           default="screen" /> 

 

    <!-- gmapping -->
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="$(arg output)">

        <param name="odom_frame" value="odom"/>
        <param name="base_frame" value="base_link"/>

        <remap from="scan" to="scan"/>

        <!--param name="map_update_interval" value="3.0"/-->
        <param name="map_update_interval" value="30.0"/>

        <!-- Set maxUrange < actual maximum range of the Laser -->
        <param name="maxRange" value="16.0"/>
        <param name="maxUrange" value="15.5"/>

        <param name="sigma" value="0.05"/>
        <param name="kernelSize" value="1"/>
        <param name="lstep" value="0.05"/>
        <param name="astep" value="0.05"/>
        <param name="iterations" value="5"/>
        <param name="lsigma" value="0.075"/>
        <param name="ogain" value="3.0"/>
        <param name="lskip" value="0"/>
        <param name="srr" value="0.1"/>         <!-- rbx1  value="0.01"/-->
        <param name="srt" value="0.2"/> 	<!-- rbx1  value="0.02"/-->
        <param name="str" value="0.1"/> 	<!-- rbx1  value="0.01"/-->
        <param name="stt" value="0.2"/>  	<!-- rbx1  value="0.02"/-->
        <param name="linearUpdate" value="0.10"/>         <!-- rbx1  value="0.5"/-->
        <param name="angularUpdate" value="0.2"/>         <!-- rbx1  value="0.436"/-->
        <param name="temporalUpdate" value="1.0"/>        <!-- rbx1  value="-1.0"/-->
        <param name="resampleThreshold" value="0.25"/>    <!-- rbx1  value="0.5"/-->
        <param name="particles" value="30"/>              <!-- rbx1  value="80"/-->

   
        <param name="xmin" value="-50.0"/>
        <param name="ymin" value="-50.0"/>
        <param name="xmax" value="50.0"/>
        <param name="ymax" value="50.0"/>
        <param name="delta" value="0.05"/>
        <param name="llsamplerange" value="0.01"/>
        <param name="llsamplestep" value="0.01"/>
        <param name="lasamplerange" value="0.005"/>
        <param name="lasamplestep" value="0.005"/>

    </node>




</launch>
